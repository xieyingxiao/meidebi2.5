<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="CardText">
        <item name="android:textSize">@dimen/card_small_text</item>
        <item name="android:textColor">@color/card_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="CardLightText" parent="@style/CardText">
        <item name="android:textColor">@color/card_light_text</item>
    </style>

    <style name="CardTitle" parent="@style/CardText">
        <item name="android:textSize">@dimen/card_large_text</item>
        <item name="android:textColor">@color/card_title_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CardTitleBold" parent="@style/CardText">
        <item name="android:textSize">@dimen/card_large_text</item>
        <item name="android:textColor">@color/card_title_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="StackTitle" parent="@style/CardText">
        <item name="android:textSize">27.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/card_title_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">300.0dip</item>
    </style>

    <style name="Item.Top" parent="@style/Item">
        <item name="android:background">#ffeeeeee</item>
    </style>

    <style name="Item.Sticky" parent="@style/Item">
        <item name="android:textAppearance">?android:textAppearanceLarge</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#ffeeeeee</item>
        <item name="android:layout_height">@dimen/sticky_height</item>
    </style>



    <style name="splash_style" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <!--<item name="android:windowTranslucentStatus">true</item>-->
        <!--<item name="android:windowTranslucentNavigation">true</item>-->

    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">false</item>
        <item name="color">@android:color/white</item>
    </style>


    <style name="NavigationStyle" parent="Widget.AppCompat.Toolbar.Button.Navigation">
         <item name="color">@android:color/white</item>
    </style>

    <style name="PageIndicatorDefaults">
        <item name="vpiTabPageIndicatorStyle">@style/Widget.TabPageIndicator</item>
    </style>

    <style name="Widget.TabPageIndicator"  >
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/text_selected_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/vpi__tab_indicator</item>
        <item name="android:paddingLeft">7.0dip</item>
        <item name="android:paddingTop">17dip</item>
        <item name="android:paddingRight">7.0dip</item>
        <item name="android:paddingBottom">17dip</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="PageIndicatorNoBg">
        <item name="vpiTabPageIndicatorStyle">@style/Widget.TabPageIndicatorNoBg</item>
    </style>

    <style name="Widget.TabPageIndicatorNoBg"  >
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@drawable/vpi_nobg_text_selected_color</item>
        <item name="android:paddingLeft">17.0sp</item>
        <item name="android:paddingTop">21.0dip</item>
        <item name="android:paddingRight">17.0sp</item>
        <item name="android:paddingBottom">21.0dip</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="CustomTabPageIndicator.Text" parent="@style/TextAppearance.AppCompat">
        <item name="android:typeface">monospace</item>
    </style>

